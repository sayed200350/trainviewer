<plist version="1.0">
<dict>
	<key>CFBundleDisplayName</key>
	<string>BahnBlitz</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>com.bahnblitz.app</string>
	<key>NSSiriUsageDescription</key>
	<string>This app uses Siri to provide voice-activated train schedule information even when the app is closed or your device is locked.</string>
	<key>NSUserActivityTypes</key>
	<array>
		<string>com.bahnblitz.app.trainquery</string>
	</array>
